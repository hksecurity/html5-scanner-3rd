{
    "manifest_version": 2,
    
    "name": "Scanner for vulnerable script",
    "description": "Scanner for vulnerable script",
    "version": "1.0",
	"options_page": "options.html",
    
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "tabs",
        "<all_urls>",
		"storage",
		"*://*/*"
    ]
}